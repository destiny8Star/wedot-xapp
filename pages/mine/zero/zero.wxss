    margin: 0 auto 50rpx
}
.share-btn{
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: space-around;
    text-align: center;
    font-size: 28rpx;
    padding:0 80rpx
}
.sf-btn image{
    width: 76rpx;
    height: 76rpx;
    display: block;
    margin: 0 auto 10rpx;
}
.q-share{
    width: 41rpx;
    height: 41rpx;
    margin:40rpx auto 0
}
.q-share image{
    width: 41rpx;
    height: 41rpx;
}
.add-adr{
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 3;
}